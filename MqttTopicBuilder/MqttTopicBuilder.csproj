<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Authors>pbouillon</Authors>
    <PackageDescription>📮 MqttTopicBuilder is a tool to build valid and verified MQTT topics</PackageDescription>
    <IsPackable>true</IsPackable>
    <PackageId>MqttTopicBuilder</PackageId>
    <PackageLicenseUrl>https://github.com/pBouillon/MqttTopicBuilder/blob/master/LICENSE</PackageLicenseUrl>
		<PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <PackageProjectUrl>https://github.com/pBouillon/MqttTopicBuilder/</PackageProjectUrl>
    <PackageTags>mqtt,topic,topics,mqtt-topics,mqtt-tools</PackageTags>
    <RepositoryType>github</RepositoryType>
    <RepositoryUrl>https://github.com/pBouillon/MqttTopicBuilder/</RepositoryUrl>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Title>MqttTopicBuilder</Title>
    <Version>1.3.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\MqttTopicBuilder.Exceptions\MqttTopicBuilder.Exceptions.csproj" />
  </ItemGroup>

</Project>
